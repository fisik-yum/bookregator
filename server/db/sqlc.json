{
    "version": "2",
    "sql": [
        {
            "schema": "schema.sql",
            "queries": "query.sql",
            "engine": "sqlite",
            "gen": {
                "go": {
                    "package": "db",
                    "out": ".",
                    "emit_json_tags": true,
                    "emit_pointers_for_null_types": true
                }
            }
        }
    ]
}
